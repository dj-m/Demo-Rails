<label for="item name">Item:</label>
<input type="text" name="item[title]"/ value="<%= @item.title %>"><br>
<label for="start date">Start Date: (YYYY-MM-DD)</label>
<input text="datetime" name="item[start_date]" value="<%= @item.start_date %>"/><br>
<label for="end date">End Date: (YYYY-MM-DD)</label>
<input type="datetime" name="item[end_date]" value="<%= @item.end_date %>"/><br>
<label for="item description">Description:</label>
<textarea name="item[description]"><%=@item.description%></textarea><br>
